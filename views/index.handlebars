<h1>Burger App</h1>


<ul class="left">
  {{#each burger}}
  	<li>
  		<p>{{this.burger_name}}</p>
      

      <button data-id="{{this.id}}" class="delquote">Delete</button>

      
  	</li>
  {{/each}}
</ul>




<h2>Add a Burger</h2>

<form class="create-form">


  <div class="form-group">
    <textarea id="quo" name="quote" rows="4" cols="40"></textarea>
  </div>

  <button type="submit">Add Burger</button>
</form>
